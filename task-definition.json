{
    "containerDefinitions": [
        {
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs-drift-dev/",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "fe"
                }
            },
            "portMappings": [
                {
                    "hostPort": 8000,
                    "protocol": "tcp",
                    "containerPort": 8000
                }
            ],
            "image": "133634153917.dkr.ecr.us-east-1.amazonaws.com/drift_frontend:latest",
            "essential": true,
            "name": "drift_frontend"
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ], 
    "memory": "2048",
    "family": "drift_frontend"
}